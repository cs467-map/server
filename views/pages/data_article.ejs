<a class="sourceLine" id="cb15-7" title="7"><span class="kw">SELECT</span> c.<span class="kw">id</span>, ST_Value(u.rast, ST_SetSRID(ST_Point(c.lon, c.lat), <span class="dv">4326</span>)) <span class="kw">as</span> uvidx <span class="kw">into</span> temp <span class="kw">table</span> juluv <span class="kw">from</span> city c, uvrasters u <span class="kw">where</span> u.filename <span class="op">=</span> <span class="st">&#39;JULY.TIFF&#39;</span>;</a>
