<div class="container-fluid h-100">
    <button id="toggle">Toggle Filters</button>
    <div class="row h-100">
        <div class="col-lg-2">
            <div id="filter" class="hidden">
                <p>Population: <span id="demo"></span></p>
                <div>
                    <input type="range" min="0" max="10000000" value="100000" class="slider" id="myRange"/>
                </div>
                <div>
                    <input type="radio" id="rating" name="markers" value="rating"
                    checked>
                    <label for="rating">rating</label>
                </div>

                <div>
                    <input type="radio" id="population" name="markers" value="population">
                    <label for="population">population</label>
                </div>

                <div>
                    <input type="radio" id="internet" name="markers" value="internet">
                    <label for="internet">internet</label>
                </div>
            </div>
        </div>

        <div id="content" class="col-lg-12 h-100">
            <div id="map"></div>
        </div>
    </div>

</div>
<script src="main.bundle.js"></script>
<script src="filter.js"></script>
